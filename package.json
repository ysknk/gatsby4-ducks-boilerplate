{
  "name": "gatsby4 ducks boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby4 ducks boilerplate",
  "author": "ysknk",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "init": "npm run clean",
    "dev": "npm run develop",
    "dev:preview": "cross-env GATSBY_ACTIVE_ENV=preview npm run develop",
    "dev:production": "cross-env GATSBY_ACTIVE_ENV=production npm run develop",
    "develop": "npm run init && gatsby develop",
    "start": "npm run develop",
    "build": "npm run init && gatsby build --prefix-paths",
    "generate": "npm run build",
    "generate:preview": "cross-env GATSBY_ACTIVE_ENV=preview npm run generate",
    "generate:production": "cross-env GATSBY_ACTIVE_ENV=production npm run generate",
    "clean": "gatsby clean",
    "typecheck": "tsc"
  },
  "dependencies": {
    "gatsby": "^4.5.4",
    "gatsby-plugin-google-tagmanager": "^4.5.0",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-layout": "^3.8.0",
    "gatsby-plugin-manifest": "^4.5.2",
    "gatsby-plugin-offline": "^5.7.0",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-react-redux": "^1.1.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-plugin-sitemap": "^5.5.0",
    "gatsby-plugin-stylus": "^4.6.0",
    "gatsby-plugin-stylus-resources": "^2.0.3",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-source-filesystem": "^4.5.2",
    "gatsby-transformer-sharp": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.22",
    "@types/stylus": "^0.48.36",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cross-env": "^7.0.3",
    "dotenv": "^14.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "typescript": "^4.5.5"
  }
}
